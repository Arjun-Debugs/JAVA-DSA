import java.util.*;
class Main{
  public static void main(String args[]){
    Scanner s = new Scanner(System.in);
    int n = s.nextInt();
    int res = fib(n);
    System.out.print(res);
  }
  public static int fib(int n){
    if(n==1) return 0;
    if(n==2) return 1;
    else return fib(n-1)+fib(n-2);
  }
}
